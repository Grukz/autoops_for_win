<?xml version="1.0" encoding="utf-8"?>
<appops>
	<app>kcxp</app>
	<actions>
		<start>
			<startcmd>c:\abc\python.exe</startcmd>
			<steps>
				<step action="getwin" func="确定异常窗口">
						<title>PropTrans</title>
						<clsname></clsname>
						<id>errwin</id>
				</step>
				<step action="btnclick" func="点击确定按钮，跳过异常">
						<title>确定</title>
						<ref_win_id>errwin</ref_win_id>
				</step>
				<step action="getwin" func="确定主程序窗口">
						<title>PROP2000通用交易接口</title>
						<clsname></clsname>
						<id>mainwin</id>
				</step>
				<step action="moveto" func="将鼠标移到'系统'菜单">
						<point x="33" y="37"></point>
						<ref_win_id>PropTrans</ref_win_id>
				</step>
				<step action="lclick" func="点击'系统'菜单，以弹出二级菜单">
				</step>
				<step action="moveto" func="将鼠标移动到'启动服务'子菜单">
						<point x="33" y="57"></point>
						<ref_win_id>mainwin</ref_win_id>
				</step>
				<step action="lclick" func="点击'启动服务'菜单，以弹出'登录'窗口">
				</step>
				<step action="getwin" func="确定登录窗口">
						<title>登录</title>
						<clsname></clsname>
						<id>loginwin</id>
				</step>
				<step action="moveto" func="将鼠标移动到'用户名'输入框">
						<point x="80" y="53"></point>
						<ref_win_id>loginwin</ref_win_id>
				</step>
				<step action="lclick" func="点击鼠标，'用户名'输入框获得焦点">
				</step>
				<step action="settext" func="往'用户名'输入框输入用户名">
						<text>tyjk</text>
				</step>
				<step action="moveto" func="将鼠标移动到'密码'输入框">
						<point x="80" y="80"></point>
						<ref_win_id>loginwin</ref_win_id>
				</step>
				<step action="lclick" func="点击鼠标，'密码'输入框获得焦点">
				</step>
				<step action="settext" func="往'密码'输入框输入密码">
						<text>tyjk</text>
				</step>
				<step action="btnclick" func="点击确定按钮，启动服务">
						<title>确定</title>
						<ref_win_id>loginwin</ref_win_id>
				</step>
			</steps>
		</start>
		<stop></stop>
	</actions>
</appops>